{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "shell:default",
    "store:allow-get",
    "store:allow-set",
    "store:allow-save",
    "store:allow-load",
    "fs:default",
		"fs:allow-remove",
		"fs:allow-read-dir",
		"fs:allow-read-text-file",
		"fs:allow-app-write",
		"fs:allow-exists",
    {
      "identifier": "fs:scope",
      "allow": [
        {
          "path": "**",
          "requireLiteralLeadingDot": false
        },
        {
          "path": "$HOME/**",
          "requireLiteralLeadingDot": false
        },
        {
          "path": "$APPCONFIG/**",
          "requireLiteralLeadingDot": false
        },
        {
          "path": "$APPDATA/**",
          "requireLiteralLeadingDot": false
        },
        {
          "path": "$APPLOCALDATA/**",
          "requireLiteralLeadingDot": false
        },
        {
          "path": "$CONFIG/**",
          "requireLiteralLeadingDot": false
        },
        {
          "path": "$DATA/**",
          "requireLiteralLeadingDot": false
        }
      ]
    },
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "exec-sh",
          "cmd": "sh",
          "args": [
            "-c",
            {
              "validator": "\\S+"
            }
          ],
          "sidecar": false
        }
      ]
    },
    "cli:default",
    "updater:default"
  ]
}